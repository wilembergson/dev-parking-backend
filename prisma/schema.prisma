// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id
  name     String
  email    String
  password String
  age      Int

  @@map("users")
}

model Car {
  id    String @id
  name  String
  brand String
  board String
  price Int

  @@map("cars")
}
